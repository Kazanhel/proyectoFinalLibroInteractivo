<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.denis.mercenario">

    <application
        android:name=".orientacion.Orientacion"
        android:allowBackup="true"
        android:launchMode="singleTask"
        android:icon="@mipmap/proxy_form"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/proxy_form"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".presenter.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".presenter.HistoriaActivity"
            android:parentActivityName=".presenter.MainActivity">
        </activity>
    </application>

</manifest>